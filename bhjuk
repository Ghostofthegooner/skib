cvvimport random
import time

cat_first_names = ["Fluffy", "Whisker", "Captain", "Sir", "Lady", "Meow", "Purr"]
cat_last_names = ["McFur", "Whiskertons", "Paws", "Fuzz", "Snuggles", "Von Meowenstein"]

titles = ["The Brave", "The Sleepy", "The Loud", "The Invisible", "The Destroyer of Furniture"]

def generate_cat_name():
    first = random.choice(cat_first_names)
    last = random.choice(cat_last_names)
    title = random.choice(titles)
    return f"{first} {last}, {title}"

def meow_animation():
    print("Summoning the cat...")
    for _ in range(3):
        print(".")
        time.sleep(0.5)
    print("ğŸ¾ MEOW ğŸ¾\n")

def main():
    meow_animation()
    cat_name = generate_cat_name()
    print(f"Your cat's official name is: {cat_name}")

if __name__ == "__main__":
    main()
